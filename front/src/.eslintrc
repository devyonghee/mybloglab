{
  "extends": [
    "eslint:recommended",
    "react-app",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "flowtype",
    "import",
    "jsx-a11y",
    "react",
    "react-hooks"
  ],
  "rules": {
    "no-console": "off",
    "jsx-a11y/no-autofocus": [
      2,
      {
        "ignoreNonDOM": true
      }
    ],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}